{
    "name": "nbayramberdiyev/slim-4-starter",
    "type": "project",
    "description": "A skeleton for a simple PHP application built on Slim Framework 4.",
    "keywords": ["slim","framework","slimframework","slimphp"],
    "authors": [
        {
            "name": "N'Bayramberdiyev",
            "email": "nbayramberdiyev@outlook.com",
            "homepage": "https://nbayramberdiyev.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.0",
        "php-di/php-di": "^6.0",
        "slim/twig-view": "^3.0",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0",
        "zeuxisoo/slim-whoops": "0.7.*"
